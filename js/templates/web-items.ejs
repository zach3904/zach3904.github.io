<% $.each(webProjects, function (i, project) { %>

    <li data-projectID="<%= i %>">
        <img src="images/design/full/<%= project.imgName %>/<%= project.imgName %>-1.jpg" alt="<%= project.title %>" />
        <div class="info-hover">
            <h3><%= project.title %></h3>
            <a href="<%= project.url %>" class="view-site" target="_blank">View Site</a>
            <a href="#webModal" class="view-details">View Details</a>
        </div>
        <div class="thumb-dim"></div>
    </li>

<% }); %>